global:
  resolve_timeout: 5m
  smtp_require_tls: true
route:
  receiver: default-receiver
  group_wait: 10s
  group_interval: 5m
  repeat_interval: 3h
receivers:
  - name: default-receiver
{% if prometheus_enable_jira | bool %}
  - name: 'jira'
    webhook_configs:
    - url: 'http://localhost:9097/alert'
{% endif %}
templates: []
